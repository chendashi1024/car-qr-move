{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "APP_URL": "https://car.qr.move.chendashi.cn"
  },
  "build": {
    "env": {
      "VITE_API_URL": "https://car.qr.move.chendashi.cn/api"
    }
  }
}